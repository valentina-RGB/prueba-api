{
  "name": "encafeinados-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "install:all": "npm install && npm run db:migrate:dev && npm run db:seed:dev",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:test": "cross-env NODE_ENV=test nest start",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "db:migrate:create": "node scripts/create-migration.js",
    "db:migrate": "cross-env NODE_ENV=development npx ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:run -d src/config/data-source.ts",
    "db:migrate:prod": "cross-env NODE_ENV=production npx ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:run -d src/config/data-source.ts",
    "db:migrate:revert:dev": "cross-env NODE_ENV=development npx ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:revert -d src/config/data-source.ts",
    "db:migrate:revert:test": "cross-env NODE_ENV=test npx typeorm migration:revert",
    "db:migrate:revert:prod": "cross-env NODE_ENV=production npx typeorm migration:revert",
    "db:seed": "cross-env NODE_ENV=development npx ts-node -r tsconfig-paths/register src/core/infrastructure/database/seeder.ts",
    "db:seed:prod": "env NODE_ENV=production npx ts-node -r tsconfig-paths/register src/core/infrastructure/database/seeder.ts",
    "test:unit": "cross-env NODE_ENV=test jest --config ./test/jest-unit-config.ts",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e-config.ts",
    "test:watch": "jest --config jest-unit-config.ts --watch",
    "test:unit:cov": "cross-env NODE_ENV=test jest --config ./test/jest-unit-config.ts --coverage",
    "test:e2e:cov": "cross-env NODE_ENV=test jest --config ./test/jest-e2e-config.ts --coverage",
    "db:test:reset": "cross-env NODE_ENV=test npx typeorm schema:drop && npm run db:migrate:test && npm run db:seed:test",
    "typeorm:migrate:test": "cross-env NODE_ENV=test npx ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:run -d src/config/data-source.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^9.7.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.0.3",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^1.37.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "google-auth-library": "^9.15.1",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.6.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.6",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.7",
    "@types/node": "^22.10.7",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
